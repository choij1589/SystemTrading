# Secrets Configuration Template
#
# IMPORTANT:
# 1. Copy this file to secrets.yaml
# 2. Fill in your actual API keys and tokens
# 3. NEVER commit secrets.yaml to git
# 4. Add secrets.yaml to .gitignore

# Binance API Credentials
binance:
  api_key: "your_binance_api_key_here"
  secret_key: "your_binance_secret_key_here"

# Telegram Bot (for notifications)
telegram:
  enabled: false
  bot_token: "your_telegram_bot_token_here"
  chat_id: "your_telegram_chat_id_here"

# Optional: Other Exchange APIs
# bithumb:
#   api_key: ""
#   secret_key: ""
